<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>

<shortcuts>

    <skipIfNotSupported/>

    <programGroup defaultName="JSAMS" location="applications"/>

      <shortcut
        name="JSAMS"
        programGroup="yes"
        desktop="yes"
        applications="no"
        startMenu="yes"
        startup="no"
        target="$INSTALL_PATH\JSAMS-1.0.2-SNAPSHOT.jar"
        commandLine=""
        description="Java Simplified Accounting Management System"
        iconFile="$INSTALL_PATH\jsams.ico"
        iconIndex="0"
        initialState="noShow">

         <createForPack name="JSAMS"/>

       </shortcut>
 

    <shortcut
           name="JSAMS Uninstaller"
           programGroup="yes"
           desktop="no"
           applications="no"
           startMenu="no"
           startup="no"
           target="$INSTALL_PATH\Uninstaller\uninstaller.jar"
           commandLine=""
           iconFile="%SystemRoot%\system32\SHELL32.dll"
           iconIndex="31"
           description="JSAMS Example">

        <createForPack name="JSAMS"/>
    </shortcut>

</shortcuts>
